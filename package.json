{
  "name": "icarus",
  "displayName": "Icarus",
  "description": "For Icarus",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main":"src/icarus.js",
  "type": "module",
  "contributes": {
    "commands": [
      {
        "command": "icarus.version",
        "title": "Icarus Version"
      }
    ],
    "languages": [
    {
      "id": "icarus",
      "aliases": ["Icarus", "icarus"],
      "extensions": [".py",".ica"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "icarus",
      "scopeName": "source.icarus",
      "path": "./syntaxes/MagicPython.tmLanguage.json"
    },
    {
      "scopeName": "source.regexp.icarus",
      "path": "./syntaxes/MagicRegExp.tmLanguage.json"
    }
    
  ]
  }
}
